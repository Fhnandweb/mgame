body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td {
  margin:0;
  padding:0;
}
body{
  background:#ddf;
  color:#000;
  font-size:14px;
  font-family: 'Microsoft yahei',Verdana, Arial, Helvetica, sans-serif;
}
div.wrap{
  max-width: 1920px;
  min-width: 1280px;
  overflow: hidden;
  position: relative;
  margin: 0 auto;
}
ol,ul,li {
  list-style:none;
}
a{
    text-decoration: none;
    color:#000
}
/* main 的头 */
header{
  height: 500px;
  background: #fff;
  width: 100%;
  position: relative;
}
header>.mhead{
  height: 100%;
  width: 100%;
  background: url("../img/bg/h-3.png")no-repeat;
  background-color: #fff;
  position: relative;
}
header>.mhead>ul{
  height: 70px;
  width: 100%;
  line-height: 70px;
  background: rgba(30, 30,30, .7);
  position: absolute;
  bottom: 0;
}
header>.mhead>ul>li{
  float: left;
  font-size: 25px;
  font-weight: bold;
  color: #fff;width: 10%;
  text-align:center
}
header>.mhead>ul>li:first-child{
  margin-left: 45px;
  width: 6%
}
header>.mhead>ul>li:nth-child(2){
  width: 6%
}
header>.mhead>ul>li a{
  color: #fff;
}
header>.mhead>ul>li a:hover{
  color: rgb(250, 186, 91);
}
.gamebase{
  width: 320px;
  height: 200px;
  background: url('../img/index/logo.png')no-repeat;
  position: absolute;
  background-size: cover;
  right: 10%;top:20%
}
/* 其他 的头 */

header>.mhead2{
  height: 100%;
  width: 100%;
  background: url("../img/bg/h-3.png")no-repeat;
  background-color: #fff;
  position: relative;
  background-position-x: -20px
}
header>.mhead2>ul{
  height: 70px;
  width: 100%;
  line-height: 70px;
  background: rgba(30, 30,30, .7);
  position: absolute;
  bottom: 0;
}
header>.mhead2>ul>li{
  float: left;
  font-size: 20px;
  font-weight: bold;
  color: #fff;width: 12%;
  margin-left: 20px
}
header>.mhead2>ul>li:first-child{
  margin-left: 45px;
  width: 6%
}
header>.mhead2>ul>li:nth-child(2){
  width: 6%
}
header>.mhead2>ul>li a{
  color: #fff;
  text-decoration: none
}
header>.mhead2>ul>li a:hover{
  color: rgb(250, 186, 91);
}
.gamebase2{
  width: 320px;
  height: 200px;
  background: url('../img/index/logo.png')no-repeat;
  position: absolute;
  background-size: cover;
  left: 2%;top:5%
}
/* 登录 注销 */
header>.mhead ul>li.tabli{
  float: right;
  height: 70px;
  line-height: 70px;
}
header>.mhead2 ul>li.tabli2{
  float: right;
  height: 70px;
  line-height: 70px;
  width: 15%
}
.report,.sout{
  /* background:#0069d9; */
  color:#fff;
  border:0;
  width: 40px;
  height: 50%;
  font-size: 15px;
  border-radius: 5px
}
.sout{
  background:#0069d9;
}
.judge,.judge2{
  font-size: 15px;
}
/* 其他 foot*/
